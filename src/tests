// -*- c -*-

// This is a line comment.

/*
 * This is a block comment.
 */

int assert(int expected, int actual, char *code) {
    if (expected == actual) {
        printf("%s => %d\n", code, actual);
    } else {
        printf("%s => %d expected but got %d\n", code, expected, actual);
        exit(1);
    }
}

int main() {
    assert(0,({0&1;}),"{0&1;}");
    assert(1,({3&1;}),"{3&1;}");
    assert(3,({7&3;}),"{7&3;}");
    assert(10,({-1&10;}),"{-1&10;}");
    assert(1,({0|1;}),"{0|1;}");
    assert(0,({0^0;}),"{0^0;}");
    printf("OK\n");
    return 0;
}
